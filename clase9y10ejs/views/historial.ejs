<h2 style="color:crimson;">
    browser history</h2>

<% const hayProductos=productos.length> 0; %>
    <% if(hayProductos){ %>
        <div class="table-responsive">
            <table class="table table-dark">
                <tr style="color: yellow;">
                    <th>Id</th>
                    <th>Title</th>
                    <th>Price</th>
                    <th>Thumbnail</th>
                </tr>
                <% for (const producto of productos) {%>
                    <tr>
                        <td>
                            <%=producto.id%>
                        </td>
                        <td>
                            <%=producto.title%>
                        </td>
                        <td>
                            <%=producto.price%>
                        </td>
                        <td>
                            <%=producto.thumbnail%>
                        </td>
                    </tr>
                    <%};%>
            </table>
            <a class="btn btn-primary" href="http://localhost:8080/" role="button">Back to form</a>
        </div>
        <% } else{ %>
            <h3 class="alert alert-danger">no se encontraron datos</h3>
            <% } %>